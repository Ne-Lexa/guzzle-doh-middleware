{
    "name": "nelexa/guzzle-doh-middleware",
    "description": "A Dns over Https (DoH) middleware for Guzzle >= 6.0",
    "keywords": [
        "doh",
        "dns-over-https",
        "Guzzle",
        "middleware",
        "dns"
    ],
    "type": "library",
    "require": {
        "php": "^7.1 | ^8.0",
        "ext-curl": "*",
        "daverandom/libdns": "^2.0",
        "guzzlehttp/guzzle": "^6.3 | ^7.0",
        "phpunit/phpunit": "^7 | ^8.5.23 | ^9",
        "psr/cache": "^1.0 | ^2.0 | ^3.0",
        "psr/log": "^1.0 | ^2.0 | ^3.0",
        "psr/simple-cache": "^1.0 | ^2.0 | ^3.0"
    },
    "require-dev": {
        "symfony/var-dumper": "*",
        "symfony/cache": ">=4.4",
        "vimeo/psalm": "^4.22"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ne-Lexa",
            "email": "alexey@nelexa.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nelexa\\Doh\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nelexa\\Doh\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "symfony/cache": "Install the PSR-6 or PSR-16 caching implementation"
    }
}
